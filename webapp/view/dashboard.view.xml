        <HBox id="_IDGenhHBox1">
        <HBox id="_IDGenHBox213" 
        class="hbox1IndiseDashBoard">
            <viz:VizFrame
                width="450px"
                height="400px"
                class="chartStyles"
                id="_IDGenVizFrame1"
                vizProperties="{title: {text: 'Monthly Invoices'}, plotArea: {dataLabel: {visible: true, type: 'value'}, dataPoint: {stroke: {visible: true}}}}"
                vizType="donut"
            >
                <viz:dataset>
                    <viz.data:FlattenedDataset
                        id="_IDGenFlattenedDataset2"
                        data="{/}"
                    >
                        <viz.data:dimensions>
                            <viz.data:DimensionDefinition
                                id="_IDGenDimensionDefinition2"
                                name="Month"
                                value="{Month}"
                            />
                        </viz.data:dimensions>
                        <viz.data:measures>
                            <viz.data:MeasureDefinition
                                id="_IDGenMeasureDefinition2"
                                name="Invoices"
                                value="{Invoices}"
                            />
                        </viz.data:measures>
                    </viz.data:FlattenedDataset>
                </viz:dataset>
                <viz:feeds>
                    <viz.feeds:FeedItem
                        id="_IDGenFeedItem1"
                        type="Dimension"
                        values="Month"
                        uid="color"
                    />
                    <viz.feeds:FeedItem
                        id="_IDGenFeedItem2"
                        type="Measure"
                        values="Invoices"
                        uid="size"
                    />
                </viz:feeds>
            </viz:VizFrame>
</HBox>
